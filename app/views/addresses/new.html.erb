<%= form_for([student, address]) do |f| %>
  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.label :street %>
  <%= f.text_field :street %>

  <%= f.submit %>
<% end %>
